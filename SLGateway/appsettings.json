{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "/app/Logs/Log-.txt",
                    "rollingInterval": "Day"
                }
            }
        ]
    },
    "AllowedHosts": "*",
    "SecondLifeHost": "secondlife.io",
    "ConnectionStrings": {
        "Storage": ""
    },
    "DatabaseName": "SLGatewayProd",
    "Auth0": {
        "Domain": "",
        "ClientId": "",
        "ClientSecret": ""
    },
    "MaxEventsCache": 25,
    "LongPollTimeoutSec": 20
}
