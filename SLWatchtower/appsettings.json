{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "/app/Logs/Log-.txt",
                    "rollingInterval": "Day"
                }
            }
        ]
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "Storage": ""
    },
    "DatabaseName": "SLWatchtowerProd",
    "GatewayUrl": "https://slgateway.herokuapp.com",
    "Auth0": {
        "Domain": "",
        "ClientId": "",
        "ClientSecret": ""
    }
}
